{
  "name": "@dddforum/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prestart": "cd ../shared && npm run build",
    "start": "vite",
    "start:dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:unit": "jest -c jest.config.unit.ts",
    "test:unit:dev": "jest -c jest.config.unit.ts --watch",
    "test:e2e": "jest -c jest.config.e2e.ts",
    "test:e2e:dev": "jest -c jest.config.e2e.ts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@dddforum/api": "*",
    "@dddforum/core": "*",
    "@dddforum/errors": "*",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "firebase": "^11.3.1",
    "history": "^5.3.0",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^5.4.5",
    "react-moment": "^1.1.3",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.10",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "esbuild": "0.24.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^22.10.0",
    "vite": "^5.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
